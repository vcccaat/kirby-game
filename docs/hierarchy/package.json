{
  "name": "hierarchy-inheritance-tree",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:tree": "cd hierarchy-graph && yarn run build && mkdir -p ../productionDocs && mv build ../productionDocs && cd ../productionDocs && mv build hierarchy",
    "build:docs": "mkdir -p ./productionDocs && cp -R docs ./productionDocs",
    "build": "rm -rf ./productionDocs && npm-run-all build:*",
    "postinstall": "(cd generator && yarn); (cd hierarchy-graph && yarn);",
    "tree:generateHierarchyData": "cd generator && ts-node generateHierarchyData.ts",
    "tree:serveFrontend": "cd hierarchy-graph && PORT=2222 yarn run start",
    "serveTree": "npm-run-all tree:*",
    "serveDocs": "http-server --o=/docs/index.html --port=5500",
    "start": "npm-run-all --parallel serveTree serveDocs",
    "startCache": "npm-run-all --parallel tree:serveFrontend serveDocs"
  },
  "dependencies": {},
  "devDependencies": {
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5"
  }
}
